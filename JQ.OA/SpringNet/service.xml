<?xml version="1.0" encoding="utf-8" ?>

<objects xmlns="http://www.springframework.net">
  <description >An example for demonstrating simple IoC features. </description>
  <object name="UserInfoService" type="SpringNet.UserInfoService, SpringNet">
    <property name="UserName" value="Jason"></property>
    <property name="Person" ref="Person"></property>
  </object>
  <object name="Person" type="SpringNet.Person, SpringNet">
    <property name="Age" value="20"></property>
  </object>

</objects>